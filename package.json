{
  "name": "continuous-delivery",
  "version": "1.0.0",
  "description": "A practice repository for testing and deployment.",
  "main": "index.js",
  "engines": {
    "node": "8.8.0"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "bundle": "browserify ./client/index.js -t babelify -o ./client/main.js",
    "bundle:watch": "watchify ./client/index.js -vd -t babelify -o ./client/main.js",
    "watch": "nodemon .",
    "test": "mocha test/",
    "tdd": "mocha test/ -w -R min",
    "cover": "nyc --reporter=lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngmlee/continuous-delivery.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/youngmlee/continuous-delivery/issues"
  },
  "homepage": "https://github.com/youngmlee/continuous-delivery#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "watchify": "^3.9.0"
  }
}
